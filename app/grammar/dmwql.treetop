
# elements.inject {|sum, n| sum + n.num}
# elements.inject {|prod, n| prod * n.num} 

grammar Arithmetic
  rule additive
    first:multitive second:( '+' multitive )* <AdditiveLiteral>
  end

  rule multitive
    first:primary second:( [*/%] primary )* <MultitiveLiteral>
  end

  rule primary
    number / '(' additive ')' <PrimaryLiteral>
  end

  rule number
    '-'? [1-9] [0-9]* <NumberLiteral>
  end
end
