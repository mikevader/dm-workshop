<div class="container page page-preview">
  <div class="row">
    <% @pages.each do |page| -%>
        <% page.card_nodes.each do |card_node| -%>
            <div class="col-xs-6">
              <% card = card_node.card %>
              <%= render 'shared/card_detail', card: card.card_data, position_style: card_style(card_node) %>
            </div>
        <% end -%>
    <% end -%>
  </div>
</div>
